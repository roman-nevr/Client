<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ru.rubicon.client">

    <uses-permission
        android:name="android.permission.INTERNET"
        />
    <application
        android:name=".Basement"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.GitHubActivity"
            android:label="GitHub">
        </activity>
        <activity
            android:name=".ui.RxActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".ui.ServerConnectionTest"
            android:label="Moon">
        </activity>
        <activity
            android:name=".ui.MetadataUi"
            android:label="Табличка">
        </activity>
        <activity
            android:name=".ui.SimpleTextActivity">
        </activity>
        <activity
            android:name=".ui.Odinesnik">
        </activity>
        <activity
            android:name=".ui.OdinesnikTextActivity">
        </activity>
    </application>


</manifest>
